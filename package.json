{
	"name": "livestudio",
	"version": "0.0.1",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "electron .",
		"debug": "electron . --debug",
		"build": "npm run sass && electron-rebuild",
		"sass": "sass static/sass/:static/styles/ -s compressed --update",
		"sass-watch": "sass static/sass/:static/styles/ -s compressed --update --watch --poll",
		"docs": "jsdoc -c jsdoc_conf.json src/",
		"package": "electron-packager . LiveStudio --overwrite --prune=true --icon=icons/icon.ico",
		"gen-ico": "ffmpeg -i static/assets/square_logo-256.png icons/icon.ico"
	},
	"author": "scoutchorton",
	"license": "ISC",
	"dependencies": {
		"material-design-icons": "^3.0.1",
		"vue": "^2.6.14"
	},
	"devDependencies": {
		"demolivestudiomodule": "file:../DemoLiveStudioModule",
		"electron": "^13.2.3",
		"electron-packager": "^15.3.0",
		"electron-rebuild": "^2.3.5",
		"sass": "^1.37.5"
	}
}
