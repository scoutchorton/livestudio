{
  "name": "livestudio",
  "version": "0.0.0-alpha.0",
  "main": "dist/api/LiveStudio.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:api && npm run build && electron index.js",
    "start:debug": "npm run build:api && npm run build:debug && electron index.js",
    "build": "ng build --base-href ./",
    "build:debug": "ng build --base-href ./ --configuration development",
    "build:api": "tsc -p tsconfig.api.json",
    "watch": "ng build --watch --configuration development",
    "prepare": "npm run build:api"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "electron": "^16.0.5",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
