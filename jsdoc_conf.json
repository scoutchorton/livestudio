{
	"opts": {
		"destination": "docs/",
		"recurse": true,
		"template": "node_modules/tui-jsdoc-template",
		"readme": "README.md"
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"default": {
			"staticFiles": {
				"include": [
					"static/assets/white-logo.svg"
				]
			}
		},
		"logo": {
			"url": "white-logo.svg",
			"link": "https://github.com/scoutchorton/livestudio"
		},
		"useCollapsibles": true,
		"footerText": "<a href=\"https://github.com/scoutchorton/livestudio\"><code>npm i scoutchorton/livestudio</code></a>"
	}
}
